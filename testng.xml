<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<listeners>
		<listener class-name="Academy.listeners">
		</listener>
		<listener class-name="resources.ExtentReporterNG">
		</listener>

	</listeners>
	<test thread-count="5"
		name="Validating Navigation Bar is presnt or not">
		<classes>
			<class name="Academy.validateNavigation" />
		</classes>
	</test>

	<test name="Test">
		<classes>
			<class name="Academy.AppTest" />
		</classes>
	</test>

	<test name="Validating Title is presnt or not">
		<classes>
			<class name="Academy.validateTitle" />
		</classes>
	</test>

	<test name="Validating User Login">
		<classes>
			<class name="Academy.HomePage" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
